  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.14.6"
  },
  "dependencies": {
    "bcryptjs": "",
    "body-parser": "*",
    "concurrently": "*",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "express": "*",
    "express-session": "*",
    "mongoose": "*",
    "multer": "*",
    "passport": "*",
    "passport-http": "*",
    "passport-local": "*",
    "socket.io": "*"
  }
}
