  "license": "MIT",
  "devDependencies": {
    "nodemon": "^1.17.2"
  },
  "dependencies": {
    "bcryptjs": "",
    "body-parser": "*",
    "concurrently": "*",
    "connect-mongo": "^2.0.1",
    "cookie-parser": "^1.4.3",
    "express": "*",
    "express-session": "*",
    "json-pretty-html": "^1.1.2",
    "mongoose": "*",
    "multer": "*",
    "passport": "*",
    "passport-facebook": "^2.1.1",
    "passport-google-oauth20": "^1.0.0",
    "passport-http": "*",
    "passport-local": "*",
    "socket.io": "*",
    "twilio": "^3.14.0"
  }
}
